<h1>Create a lunch!</h1>
<form action="/lunches/new" method="get">
  <input type="text" name="new_lunch">
  <input type="submit" value="Create lunch">
</form>

<h1>All lunches:</h1>
<ul>
  <% @lunches.each do |lunch| %>
  <li>
    <%= lunch['lunch_name']%> ——— <%= lunch['lunch_name']%>
  </li>
  <% end %>
</ul>
