
<form action="/users/new" method="get">
  <button class="new_c" type=submit>New Cawer</button>
</form>


<ul>
  <% @users.each do |user| %>
    <li>
      <div class="x">
      <a class="nameit" href="/users/<%=user.id%>">
        <%= user.username %>
      </a>
      <% user.caws.each do |caw| %>
        <p><%=caw.message%></p>
      <% end %>
    </div>
    </li>
  <% end %>

</ul>
