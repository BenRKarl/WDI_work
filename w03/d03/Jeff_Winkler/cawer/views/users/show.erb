
<h1>
  <%= @user.username %>
</h1>
<br/>

<% @user.caws.each do |caw| %>
  <div class="list">
    <div class="one_m">
      <%= caw.message %>
    </div>
    <form class="del" action="/users/<%=caw.user_id%>/caws/<%=caw.id%>" method="post">
      <input type="hidden" name="_method" value="delete">
      <button class="new_d" type="submit">Delete</button>
    </form>
  </div>
<% end %>

    <a href="/users/<%=@user.id%>/caws/new"><h1>Caw</h1></a>

