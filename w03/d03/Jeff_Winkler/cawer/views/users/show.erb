
<h1>
  <%= @user.username %>
</h1>
<br/>

<% @user.caws.each do |caw| %>
  <div class="list">
    <%= caw.message %>
    <form action="/users/<%=caw.user_id%>/caws/<%=caw.id%>" method="post">
      <input type="hidden" name="_method" value="delete">
      <button type="submit">Delete</button>
    </form>
  </div>
<% end %>
