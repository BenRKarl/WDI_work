<h1>FRIENDS</h1>
<table>
<% @friends.each do |friend| %>
<tr>
  <td>
    <img src="<%= friend.image_url %>" alt="">
  </td>
  <td>
    <%= friend.name %>
  </td>
</tr>
<td>
<form action="/friends/<%=friend.id%>/edit"
  method="get">
  <button type="submit">SHOW</button>
</form>
  </td>
</tr>
<% end %>
</table>
