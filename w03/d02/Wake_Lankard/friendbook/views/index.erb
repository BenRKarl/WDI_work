<h1>FRIENDS</h1>
<table>
  <% @friends.each do |friend| %>
    <tr>
      <td>
        <img src="<% friend.img_url %>" alt="">
      </td>
      <td>
        <form action="/friends/<%=friend.id%>" method="get">
          <button type="submit">SHOW</button>
        </form>


    </tr>

  <% end %>
</table>