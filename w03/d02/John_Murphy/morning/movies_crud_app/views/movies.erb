<h1>Here are your movies</h1>

<table>

  <thead>
    <tr>
      <th>Title</th>
      <th>Director</th>
      <th>Screenwriter</th>
    </tr>
  </thead>




  <tbody>
  <% @movies.each do |movie| %>
    <tr>
      <td><%= movie.title %></td>
      <td><%= movie.director %></td>
      <td><%= movie.screenwriter %></td>
    </tr>
  <% end %>

</tbody>

</table>
