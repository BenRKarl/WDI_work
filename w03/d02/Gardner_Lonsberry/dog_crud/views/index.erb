<h1>DOGS</h1>
<form action="/dogss/new" method="get">
  <button class="add-dog">
  Add Dog
</button>
</form>


<table>
  <% @dogs.each do |dog| %>
    <tr>
      <td>
        <img src="<%= dog.image_url %>" alt="">
      </td>
      <td>
        <%= dog.username %>
      </td>
      <td>
        <form action="/dogs/<%=dog.id%>" method="get">
          <button type="submit">SHOW</button>
        </form>
      </td>
      <td>
        <form action="/dogs/<%=dog.id%>/edit" method="get">
          <button type="submit">EDIT</button>
        </form>

      </td>
      <td>
        <form action="/dogs/<%=dog.id%>" method="post">
          <input type="hidden" name="_method" value="delete">
          <button type="submit">DELETE</button>
        </form>
      </td>
    </tr>
  <% end %>
</table>