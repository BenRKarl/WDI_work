<h1>Squares</h1>
<% @squares.each do |square| %>
  <a href="/squares/<%= square.id %>"><div style="width:<%= square.side_length %>px; height:<%= square.side_length %>px; border-radius:<%= square.border_radius %>px; background-color:<%= square.color %>; ">
</div></a>
<% end %>

<a href="/squares/new">
  New Square
</a>
