

<div class="all_cols">
    <div class="left_col">
    </div>
    <div class="midCol">
    </div>
    <div class="right_col inputs">
        <form action="/planets" method="post">
            <%= token_tag %>
            <button class="show_but save_but" type="submit">Save Planet</button>
            <input class="new_input" type="text" name="planet[name]" placeholder="Name">
            <input class="new_input" type="text" name="planet[image_url]" placeholder="Image Link">
            <input class="new_input" type="text" name="planet[diameter]" placeholder="Diameter">
            <input class="new_input" type="text" name="planet[mass]" placeholder="Mass">
            <input class="new_input" type="text" name="planet[life]" placeholder="Life?  True or False">
        </form>
     </div>
</div>
<form action="/planets" method="get">
    <button class="show_but back" type="submit">Back</button>
</form>
