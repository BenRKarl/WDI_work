<%= form_for @planet do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :"image_url" %>
  <%= f.text_field :image_url %>
  <%= f.label :diameter %>
  <%= f.text_field :diameter %>
  <%= f.label :mass %>
  <%= f.text_field :mass %>
  <%= radio_button_tag(:name, "true") %>
  <%= label_tag(:life, "True") %>
  <%= radio_button_tag(:life, "false") %>
  <%= label_tag(:life, "False") %>
  <%= f.submit %>
<% end %>
