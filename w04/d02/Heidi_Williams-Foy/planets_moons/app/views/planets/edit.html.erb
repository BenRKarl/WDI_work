<h1>Add Planets and Moons</h1>
<%= form_for @planet, url: {action: "update"} do |f| %>
  <%= f.text_field :moon, :placeholder => "Planet name" %>
  <%= f.number_field :mass, :placeholder => "Mass of planet" %>
  <%= f.label :name, 'M' %>
  <%= f.number_field :diameter, :placeholder => "Diameter of planet" %>
  <%= f.label :name, 'Km' %>
  <%= f.text_field :life, :placeholder => "true/false" %>
  <%= f.label :name, 'Life?' %>
  <%= f.submit "Update" %>
<% end %>